{
  "name": "homebridge-samygo",
  "version": "0.0.1",
  "description": "HomeBridge SamyGO plugin for remote control SamyGO Samgung TV supported",
  "license": "ISC",
  "author": "pippolino",
  "keywords": [
    "homebridge-plugin",
    "homebridge",
    "samygo",
    "samsung",
    "homekit",
    "siri"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/pippolino/homebridge-samygo.git"
  },
  "bugs": {
    "url": "https://github.com/pippolino/homebridge-samygo/issues"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=6.9.5",
    "homebridge": ">=0.4.27"
  },
  "dependencies": {
    "xml-library": "^0.0.3",
    "xmldoc": "^1.1.0"
  },
  "scripts": {
    "start": "homebridge --plugin-path .",
    "debug": "DEBUG=* homebridge --debug --plugin-path .",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "cleancache": "rm -rf ~/.homebridge/persist && rm -rf ~/.homebridge/accessories"
  }
}
